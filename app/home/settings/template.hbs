{{page-title 'Settings'}}
<Header>Settings</Header>

<div class='container'>
  <form {{!-- {{on 'submit' this.onSubmit}} --}}>
    <div class='mb-3 row'>
      <label for='username' class='col-sm-2 col-form-label'>Username</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='username'
          placeholder='username'
          {{!-- {{on 'input' this.onUsernameChange}} --}}
        />
      </div>
    </div>
    <div class='mb-3 row'>
      <label for='username' class='col-sm-2 col-form-label'>Email</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='email'
          placeholder='email'
          {{!-- {{on 'input' this.onEmailChange}} --}}
        />
      </div>
    </div>
    <div class='mb-3 row'>
      <label for='username' class='col-sm-2 col-form-label'>PhotoURL</label>
      <div class='col-sm-10'>
        <input
          type='text'
          class='form-control'
          id='photoURL'
          placeholder='photoURL'
          {{!-- {{on 'input' this.onPhotoURLChange}} --}}
        />
      </div>
    </div>
    <div class='mb-3 row'>
      <label
        for='inputPassword'
        class='col-sm-2 col-form-label'
      >Password</label>
      <div class='col-sm-10'>
        <input
          type='password'
          class='form-control'
          id='inputPassword'
          placeholder='password'
          {{!-- {{on 'input' this.onPasswordChange}} --}}
        />
      </div>
    </div>
    <div class='mb-3 row'>
      <div class='d-flex'>
        <button type='submit' class='btn btn-primary w-50 me-1'>SAVE</button>
        <button type='submit' class='btn btn-danger w-50 ms-1'>CANCEL</button>
      </div>

    </div>
  </form>
  <div>
  </div>
  <div>
    <span>Mam ju≈º konto. </span><LinkTo @route='login'>Zaloguj</LinkTo>
  </div>
</div>

{{outlet}}